    <title>Carrito</title>
</head>
<body>
    <!-- Navbar -->
    <%-include('./partials/headerNavbar')%>

    <section class="main-container-detail">
        <img class="image-product-detail" src="/img/juegoGOT" alt="" width="100%" height="100%">

    <div class="main-product-detail">  
        <h4 class="product-name">Juego de mesa GOT</h4>
        <p class="price-detail">$10000</p>

            <div class="quantity-and-cart-block">
